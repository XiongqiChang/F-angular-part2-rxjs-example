{
  "name": "ng-website",
  "version": "12.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 8001 --sourcemap=false --host 0.0.0.0",
    "local": "ng serve --port 3333 --sourcemap=false --host 0.0.0.0",
    "build": "ng build",
    "build:local": "ng build -e local",
    "build:qa": "ng build -e qa --output-hashing all",
    "build:uat": "ng build -e uat --output-hashing all",
    "build:integ": "ng build -e integ --output-hashing all",
    "build:prod": "ng build -e prod --output-hashing all --prod --aot false",
    "postinstall": "npm install enhanced-resolve@3.3.0",
    "test": "ng test --sourcemap=false --code-coverage --watch=false --single-run=true",
    "e2e": "ng e2e",
    "lint": "ng lint \"src/**/*.ts\"",
    "stylelint": "stylelint '**/*.scss' --syntax scss"
  },
  "lint-staged": {
    "*.scss": "stylelint --syntax=scss",
    "*.{ts,tsx}": "tslint"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.4.9",
    "@angular/compiler": "2.4.9",
    "@angular/core": "2.4.9",
    "@angular/forms": "2.4.9",
    "@angular/http": "2.4.9",
    "@angular/platform-browser": "2.4.9",
    "@angular/platform-browser-dynamic": "2.4.9",
    "@angular/router": "3.4.9",
    "@types/lodash": "4.14.66",
    "bootstrap": "4.0.0-alpha.5",
    "classlist.js": "1.1.20150312",
    "core-js": "2.4.1",
    "intl": "1.2.5",
    "lodash": "4.17.4",
    "ngx-bootstrap": "1.6.6",
    "rxjs": "5.5.2",
    "web-animations-js": "2.2.2",
    "zone.js": "0.7.8"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-rc.0",
    "@angular/compiler-cli": "2.4.9",
    "@types/jasmine": "2.5.38",
    "@types/node": "6.0.60",
    "codelyzer": "2.0.1",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.4.1",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "0.2.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "lint-staged": "4.2.1",
    "protractor": "5.1.0",
    "stylelint": "9.2.1",
    "stylelint-config-standard": "18.2.0",
    "ts-node": "2.0.0",
    "tslint": "4.5.1",
    "typescript": "2.1.5"
  }
}
