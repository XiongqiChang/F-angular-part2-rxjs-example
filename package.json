{
  "name": "ng-website",
  "version": "12.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 8001 --host 0.0.0.0",
    "local": "ng serve --port 3333 --host 0.0.0.0",
    "build": "ng build",
    "build:local": "ng build -e local",
    "build:qa": "ng build -e qa --output-hashing all",
    "build:uat": "ng build -e uat --output-hashing all",
    "build:integ": "ng build -e integ --output-hashing all",
    "build:prod": "ng build -e prod --output-hashing all --prod --aot false",
    "test": "ng test --sourcemap=false --code-coverage --watch=false --single-run=true",
    "e2e": "ng e2e",
    "lint": "ng lint \"src/**/*.ts\"",
    "stylelint": "stylelint '**/*.scss' --syntax scss"
  },
  "lint-staged": {
    "*.scss": "stylelint --syntax=scss",
    "*.{ts,tsx}": "tslint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/cdk": "^7.0.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@types/lodash": "4.14.66",
    "bootstrap": "4.0.0-alpha.5",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.4",
    "intl": "1.2.5",
    "lodash": "^4.17.15",
    "ngx-bootstrap": "1.6.6",
    "rxjs": "6.5.4",
    "url-polyfill": "^1.1.5",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular/cli": "~6.2.9",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.3.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "4.2.1",
    "protractor": "~5.4.0",
    "stylelint": "9.2.1",
    "stylelint-config-standard": "18.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2"
  }
}
